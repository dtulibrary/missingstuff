<div class="form-group">
    <label class="col-sm-1 control-label">ID</label>
    <div class="col-sm-4 input">
	<%= values[:person_id_type] %>
    </div>
    <div class="col-sm-7 input" style="padding-left: 0px;">
	<%= values[:person_id] %>
    </div>
</div>
<div class="form-group">
    <label class="col-sm-1 control-label">First Name</label>
    <div class="col-sm-3 input">
	<%= values[:first_name] %>
    </div>
    <label class="col-sm-1 inline-label">Last Name</label>
    <div class="col-sm-3 inline-input" style="padding-left: 0px;">
	<%= values[:last_name] %>
    </div>
    <label class="col-sm-1 inline-label">Role</label>
    <div class="col-sm-3 inline-input">
	<%= values[:role] %>
    </div>
</div>
<div class="form-group">
    <label class="col-sm-1 control-label">Affiliation</label>
    <div class="col-sm-11 input">
	<%= values[:affiliation] %>
    </div>
</div>
